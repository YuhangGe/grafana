{
  "name": "grafana-hansight",
  "version": "3.0.0-beta6",
  "repository": {
    "type": "git",
    "url": "http://github.com/YuhangGe/grafana.git"
  },
  "engines": {
    "node": "0.4.x",
    "npm": "2.14.x"
  },
  "scripts": {
    "gen-html": "node ./scripts/gen-html.js --watch",
    "gen-html:build": "node ./scripts/gen-html.js --build",
    "ts": "node ./scripts/typescripts.js --watch",
    "js:build": "node ./scripts/build-js.js",
    "prejs:build": "cp -rf ./public/app ./public/vendor ./public/test ./dist",
    "postjs:build": "rm -rf ./dist/app ./dist/test ./dist/vendor",
    "sass": "node ./scripts/gen-css.js --watch",
    "sass:build": "node ./scripts/gen-css.js --build",
    "start": "concurrently 'npm run ts' 'npm run gen-html' 'npm run sass' 'npm run server'",
    "server": "s-server ./scripts/dev-server.js --liveload",
    "server:build": "s-server ./scripts/dev-server.js --build",
    "copy:npm": "node ./scripts/copy-npm.js",
    "copy:build": "cp -rf ./public/img ./public/fonts ./public/dashboards ./dist",
    "postinstall": "npm run copy:npm",
    "build": "rm -rf ./dist && mkdir ./dist && concurrently 'npm run copy:build' 'npm run js:build' 'npm run sass:build' 'npm run gen-html:build'"
  },
  "devDependencies": {
    "zone.js": "^0.6.6",
    "glob": "~3.2.7",
    "concurrently": "^2.0.0",
    "typescript": "^1.8.10",
    "node-sass": "^3.5.3",
    "systemjs-builder": "^0.15.15",
    "sinon": "1.16.1",
    "colors": "^1.1.2",
    "s-server": "^0.2.0",
    "koa-static": "^2.0.0",
    "request": "^2.72.0",
    "websocket": "^1.0.22",
    "clean-css": "^3.4.12",
    "ng-annotate": "^1.2.1",
    "uglify-js": "^2.6.2"
  },
  "dependencies": {
    "eventemitter3": "^1.2.0",
    "remarkable": "^1.6.2",
    "tether": "^1.3.1",
    "tether-drop": "^1.4.2",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.4",
    "systemjs": "0.19.24"
  },
  "private": true
}
