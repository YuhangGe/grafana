{
  "name": "grafana-hansight",
  "version": "3.0.0-beta6",
  "repository": {
    "type": "git",
    "url": "http://github.com/YuhangGe/grafana.git"
  },
  "engines": {
    "node": "0.4.x",
    "npm": "2.14.x"
  },
  "scripts": {
    "gen-css": "",
    "gen-html": "node ./scripts/gen-html.js --watch",
    "gen-html:build": "node ./scripts/gen-html.js --build",
    "ts": "node ./scripts/typescripts.js --watch",
    "sass": "node ./scripts/gen-css.js --watch",
    "sass:build": "node ./scripts/gen-css.js --build",
    "start": "concurrently 'npm run ts' 'npm run gen-html' 'npm run sass' 'npm run server'",
    "server": "s-server ./scripts/dev-server.js --liveload",
    "copy:npm": "node ./scripts/copy-npm.js",
    "postinstall": "npm run copy:npm"
  },
  "devDependencies": {
    "zone.js": "^0.6.6",
    "glob": "~3.2.7",
    "concurrently": "^2.0.0",
    "typescript": "^1.8.10",
    "node-sass": "^3.5.3",
    "systemjs-builder": "^0.15.13",
    "sinon": "1.16.1",
    "colors": "^1.1.2",
    "s-server": "^0.2.0",
    "koa-static": "^2.0.0",
    "request": "^2.72.0",
    "websocket": "^1.0.22"
  },
  "dependencies": {
    "eventemitter3": "^1.2.0",
    "remarkable": "^1.6.2",
    "tether": "^1.2.0",
    "tether-drop": "^1.4.2",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.4",
    "systemjs": "0.19.24"
  },
  "private": true
}
